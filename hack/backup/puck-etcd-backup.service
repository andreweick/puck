[Unit]
Description=Puck: etcd snapshot offsite (rsync.net)
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
EnvironmentFile=/etc/puck/backup.env
ExecStart=/usr/local/bin/puck-etcd-rsync.sh
User=root
Nice=10
IOSchedulingClass=best-effort
ProtectSystem=full
ProtectHome=true
PrivateTmp=true
